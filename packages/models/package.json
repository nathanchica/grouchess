{
    "name": "@grouchess/models",
    "version": "1.0.0",
    "description": "Zod schemas and TypeScript types for shared data models",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "typecheck": "tsc --noEmit",
        "test": "vitest run --coverage",
        "test:run": "vitest run",
        "docs:generate": "tsx ../../scripts/generate-package-docs.ts models"
    },
    "keywords": [
        "models"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "typescript": "^5.9.3",
        "vitest": "^4.0.6"
    },
    "dependencies": {
        "zod": "^4.1.12"
    }
}
