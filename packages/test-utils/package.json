{
    "name": "@grouchess/test-utils",
    "version": "1.0.0",
    "description": "Mock data factories for unit tests and other testing utils",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "typecheck": "tsc --noEmit",
        "test": "vitest run --coverage",
        "test:run": "vitest run"
    },
    "keywords": [
        "test-utils"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@grouchess/models": "workspace:*"
    },
    "devDependencies": {
        "typescript": "^5.9.3",
        "vitest": "^4.0.6"
    }
}
